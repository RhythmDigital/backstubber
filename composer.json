    },
    "require-dev": {
        "phpspec/phpspec": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "ConstantNull\\Backstubber\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mark Belotskiy"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
