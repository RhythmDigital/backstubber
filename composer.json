  "keywords": ["php", "stub", "generator", "template"],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.5",
    "bossa/phpspec2-expect": "~1.0",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ConstantNull\\Backstubber\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Mark Belotskiy"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
